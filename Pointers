#include <stdio.h>
c int main()
{
int *p, c;
c = 25;
printf("Address of c: %p\n", &c);
printf("Value of c: %d\n\n", c);
p=&c;
printf("Address of pointer p: %p\n", p);
printf("Content of pointer p: %d\n\n", *p);
c=50;
printf("Address of pointer p: %p\n", p);
printf("Content of pointer p: %d\n\n", *p);
*p=2;
printf("Address of c:%p\n",&c);
printf("Value of c:%d\n\n",c);
return 0;
}
